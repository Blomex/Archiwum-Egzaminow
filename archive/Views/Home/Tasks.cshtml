@model TasksViewModel

<div class="text-center">
    <h1 class="display-4">@Model.CourseName</h1>
    <h1 class="display-4">@Model.TasksetName - @Model.TasksetYear</h1>
</div>

@foreach (var task in Model.Tasks)
{
    <article class="card">
        <div class="card-body">
            <h3>@task.Name </h3>
            <p>
                @task.Content
            </p>
        </div>
    </article>
}